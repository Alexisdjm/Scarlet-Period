/* Image Grid */

.section-image__grid{
  
  	.container-wrap{
      
        padding-bottom: 0px;
      
      	@media screen and (max-width: 768px){
        	padding-bottom: 0px;
          
          	.icons-row__item{
                width: 100%;
              	
            }
          
        }
      
      	@media screen and (min-width: 792px){
            .icons-row__item:first-child {
                margin-left: 0;
            }

            .icons-row__item:last-child {
                margin-right: 0;
            }

            .icons-row__item{
                width: 100%;
              	padding: 0;
              	margin-left: 0;
    			margin-right: 0;
            }
        }
      
      	.grid__shop{
        	display: inline-block;
    		padding-top: 8px;
          	font-weight: normal;
            font-family: "Matter";
            letter-spacing: 0.8px;
            font-size: 12px;
          	vertical-align: middle;
          
          	i{
            	font-weight: 600;
              	padding-left: 5px;
            }
          	
          	img{
            	width: auto;
    			height: auto;  
            }
        }
      
      	.grid-img__slider{
          
          	@media screen and (max-width: 792px){
                margin-left: auto;
                margin-right: auto;
                position: relative;
                overflow: hidden;
                list-style: none;
                padding: 0;
                z-index: 1; 
            }
        }
      
        .collectionGrid-header{
			
          	padding-bottom: 10px;
          
          	@media screen and (max-width: 792px){
            	padding-bottom: 10px;  
            }
          
            h2{
                font-size: 16px;
                font-weight: normal;
                text-transform: uppercase;
                margin: 0;
                font-family: "scarlet_talk-regular";
                letter-spacing: 1px;
              
              	@media screen and (max-width: 792px){
                  	font-size: 14px;
                }
            }
        }
      
      
      	.icons-row-wrapper{
          
          	&.icons-row-wrapper.swiper-wrapper {
                margin-left: 0 !important;
                margin-right: 0 !important;
