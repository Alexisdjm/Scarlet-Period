/* Hero Slider */

.template-index {

	.bodyWrap{
      
      	.section-slideshow{
    	
            &:first-child{

                .hero{

                    @media screen and (max-width: 767px){  
                        margin-top: 170px;
                    }
                                    	
                }

            }
          
          	.slide-content__heading{
              
                   	font-size: 55px !important;
              		letter-spacing: 0.8px;
              
					@media screen and (max-width: 767px){
                       	font-size: 35px !important;
                      	letter-spacing: 0.8px;
                    }
          	}
                  
            .slide-content__buttons{
                    	
            	.button{
                          
                	@media screen and (max-width: 767px){ 
                    	width: auto;  
                  	}
               	}
          	}
          
          	.slick-slide{
              
             	.slide-inner{
                	
                  	.slide-content{
                    	
                      	@media screen and (max-width: 767px){ 
                      		position: absolute;
                        	left: 50%;
                        	top: 50%;
                        	transform: translate(-50%,-50%);
                        	margin-top: 0 !important;
                        	max-width: 100% !important;
                        }
                      
                      	.slide-content__text{
                          	
                          	margin-bottom: 30px;
                        }
                      
                      	.slide-content__buttons{
                          
                          	a.button{
                            	
                              	&:hover{
                                	background-color: #fa0d3d !important;
                                  	color: #fff !important;
                                  
                                  	@media screen and (max-width: 767px){ 
                                      	background-color: #fa0d3d !important;
                                  		color: #fff !important;
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
  
  	section[data-section-type="marquee"]{
    	//padding-top: 100px;  
    }
  
  
  	.collection-slider__wrapper{
      
   		.swiper-wrapper{
         
        	.collectionBlock{
             
              	.collectionBlock-info{
                  
                  	@media screen and (max-width: 480px){
                  		display: flex;
    					flex-direction: column;
                    }
                  
                  	h4{
                      
                      	@media screen and (max-width: 480px){
                    		padding: 0;
    						margin-left: 0;
    						margin-top: 0; 
                        }
                    }
                }
            }
        }
    }
}



/*****
 * Mega Menu
 *
*/

.dropdown_container{
  	
  	.dropdown{
      	position: absolute !important;
        width: 100%;
        z-index: 1000;
        top: 100px;
        background: #fff;
      	padding: 30px 0;
    }
  
  	.dropdown_content {
	
        display: inline-block;
        width: 100%;

        .dropdown_column{
            display: inline-block;
            padding: 15px 20px 30px 20px;
            width: 16.666%;
            vertical-align: top;
            float: left;
			text-align: left;
          
          	ul{
            	li{
                  
                  	padding-bottom: 10px;
                  
                  	a{
                        font-family: "scarlet_talk-regular";
                        font-weight: 400;
                        font-style: normal;
                        color: rgba(0,0,0,0.75);
                    }
                }
            }
          
          	ul.dropdown_title{
              
              	li{
                  
                  	padding-bottom: 20px;
                  
                  	a{
                      	font-size: 1.125em;
                      	color: #fa0d3d;
                    }
                
              	}
            }
          
            .mega-menu__image-caption{
				margin-top: 10px;
                font-size: 1.025em;
                color: rgba(0,0,0,0.75);
                font-family: "scarlet_talk-regular";
                font-weight: 400;
                font-style: normal;
            }
          
          	img{
            	width: 100%;
              	height: auto;
            }
        }
    }
}

.mobile-mega-menu{
  	display: none;
}

.dropdown_container{
	display: none;  
}

.menu-item {
  
  	&:hover{
    	
      	.dropdown_container{
        	display: block;  
        }
    }
  
  	
}


.nav-standard.nav-main{
	position: unset;

	.menu {
		position: unset;

		.menu-item {
			position: unset;

			.dropdown{
				top: 45px;
    			left: 0;
			}
		}
	}
}

