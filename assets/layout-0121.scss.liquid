.index__slideshow {
  margin-top: 79px;

  .slick-track,
  .slick-slide {
    height: 550px;
  }

  .slick-slide .slider-image--mobile {
    height:  500px;
    position: relative;
  }

  .slick-slide .slider-image--mobile img {
    height: 100%;
    width: 100%;
    object-fit: cover;
    position: absolute;
    top: 0;
    left: 0;
  }
  .slider__home {
    @media only screen and (min-width: 768px) {
      &.height-seven-fifty-height-slide {

        .slider-image {
          height: 750px;
        }
      }
      &.height-sixty-fifty-height-slide {

        .slider-image {
          height: 650px;
        }
      }
      &.height-five-fifty-height-slide {

        .slider-image {
          height: 550px;
        }
      }
      &.height-four-fifty-height-slide {

        .slider-image {
          height: 450px;
        }
      }
    }
    .slider__home-item {
      
      .slider-image-wrap {
        position: relative;
        
        .slider__overlay {
          position: absolute;
          left: 0;
          top: 0;
          bottom: 0;
          right: 0;
          z-index: 1;
        }        
        .slider-image {
          position: relative;
          overflow: hidden;
          
          @media only screen and (min-width: 768px) {          
            img.slider__img,
            svg {
              position: absolute;
              left: 0;
              right: 0;
              bottom: 0;
              top: 0;
              width: 100%;
              height: 100%;
              object-fit: cover;
            }
          }
        }
      }
      /* Text Overlay Style */
      .slider_text--overlay {
        
        .slider__content {
          position: absolute;
          top: 50%;
          left: 0;
          right: 0;
          z-index: 2;
          -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
        }
        .slider__settings-rte {
          max-width: 600px;
          margin: 0 auto;
        }
      }
      /* Column Style */
      .slider_text--flex {
        @media only screen and (min-width: 768px) {
          display: flex;
          align-items: center;
        }
        
        .slider__content {          
          padding: 20px 0;          
          @media only screen and (min-width: 768px) {
            flex: 0 0 50%;
            order: 1;
          }
          @media only screen and (min-width: 1024px) {
            flex: 0 0 33.33%;
          }
          @media only screen and (max-width: 767px) {
            text-align: center;
            padding: 30px 0; 
            
            .slider__settings-rte {
              
              h2 {
                margin-bottom: 17px;
              }
              .slider-content__buttons {
                padding-top: 5px;
              }
            }
          }
        }
        @media only screen and (min-width: 768px) {
          .slider-image-wrap  {
            order: 2;
            flex: 1;
          }
        }
      }
      .content-center {
        
        .slider__settings-rte {
          text-align: center;
        }
      }
      .content-left {
        
        .slider__settings-rte {
          text-align: left;
          margin: 0 auto 0 0;
        }
      }
      .content-right {
        
        .slider__settings-rte {
          text-align: right;
          margin: 0 0 0 auto;
        }
      }
      .colour-white {
        
        .slider__settings-rte {
          
          h2,
          h4,
          .slider-content__text {
            color: #fff;                       
          }
        }
      }
      .slider__settings-rte {
        
        h2,
        h4 {
          font-weight: normal;
          text-transform: uppercase;
          margin-bottom: 10px;
          @media only screen and (min-width: 768px) {
            margin-bottom: 20px;
          }
        }
        .slider-content__text {
          margin-bottom: 20px;
          @media only screen and (min-width: 768px) {
            margin-bottom: 25px;
          }
        }
      }
      .text-large {
        
        h2 {
          font-size: 30px;
          line-height: 44px;
          @media only screen and (min-width: 768px) {
            font-size: 50px;
            line-height: 54px;
          }
        }
        h4 {
          font-size: 22px;
          line-height: 34px;
          @media only screen and (min-width: 768px) {
            font-size: 35px;
            line-height: 49px;
          }
        }
        .slider-content__text {
          font-size: 16px;
        }
      }
      .text-normal {
        
        h2 {
          font-size: 30px;
          line-height: 34px;
          @media only screen and (min-width: 768px) {
            font-size: 40px;
            line-height: 44px;
          }
        }
        h4 {
          font-size: 18px;
          line-height: 20px;
          text-transform: unset;
          @media only screen and (min-width: 768px) {
            font-size: 24px;
            line-height: 26px;
            text-transform: unset;
          }
        }
        .slider-content__text {
          font-size: 16px;
        }
      }
      .text-small {
        
        h2 {
          font-size: 25px;
          line-height: 29px;
          @media only screen and (min-width: 768px) {
            font-size: 30px;
            line-height: 34px;
          }
        }
        h4 {
          font-size: 20px;
          line-height: 24px;
        }
        .slider-content__text {
          font-size: 16px;
        }
      }
      .slider-content__buttons {
        
        .button--primary {
          
          &:hover {
            background-color: #fa0d3d;
            border-color: #fa0d3d;
            color: #fff;
          }
        }
      }
      .slider-image-wrap.slider--has-mobile {
        @media only screen and (min-width: 768px) {
          .slider-image--mobile {
            display: none !important;
          }
        }
        @media only screen and (max-width: 767px) {
          .slider-image--desktop {
            display: none !important;
          }
        }
      }
    }
    .slick-dots li {
      margin: 0 4px;
    }
    @media only screen and (max-width: 767px) {
      .slick-dots {
        display: none !important;
      }
    }
  }
  
  .medium-up--hide {
       display: block;
     }
     .small--hide {
       display: none;
     }
  
  @media only screen and (min-width: 768px) {
     .medium-up--hide {
       display: none;
     }
     .small--hide {
       display: block;
     }
   }
}
