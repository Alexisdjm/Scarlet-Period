/** Tab section **/
.tabs-section{
  	.accordion {
        font-size: inherit;
    	width: 100%;
    	margin: 0 auto;
    	border-radius: 5px;
      	border-top: 1px solid #efefef;
    }

    .accordion-header,
    .accordion-body {
      background: white;
    }

    .accordion-header {
      padding: 20px 5px;
      background: #fff;
      color: rgba(0, 0, 0, 0.8);
      cursor: pointer;
      font-size: 1em;
      letter-spacing: .1em;
      transition: all .3s;
      text-transform: uppercase;
      
      font-size: 12px;
      line-height: 13px;
      letter-spacing: 1.2px;
    }

    .accordion__item {
        border-bottom: 1px solid #efefef;
    }

    .accordion__item .accordion__item {
      border-bottom: 1px solid #efefef;
    }

    .accordion-header:hover {
      background: #fff;
      position: relative;
      z-index: 5;
    }

    .accordion-body {
      background: #fff;
      color: rgba(0, 0, 0, 0.8);
      display: none;
    }

    .accordion-body__contents {
      padding: 10px 5px;
      font-size: 1em;
    }

    .accordion__item.active:last-child .accordion-header {
      border-radius: none;
    }

    .accordion:first-child > .accordion__item > .accordion-header {
      //border-bottom: 1px solid transparent;
    }

    .accordion__item > .accordion-header:after {
      content: "\f2c7";
      font-family: IonIcons;
      font-size: 1.2em;
      float: right;
      position: relative;
      top: -2px;
      transition: .3s all;
      transform: rotate(0deg);
      color: rgba(0, 0, 0, 0.8);
    }

    .accordion__item.active > .accordion-header:after {
      transform: rotate(-180deg);
    }

    .accordion__item.active .accordion-header {
      background: #fff;
    }
  
  	.accordion__item.active .accordion-header:after {
      content: "\f462";
      font-family: IonIcons;
      font-size: 1.2em;
      float: right;
      position: relative;
      top: -2px;
      transition: .3s all;
      transform: rotate(0deg);
      color: rgba(0, 0, 0, 0.8);
    }

    .accordion__item .accordion__item .accordion-header {
      background: #fff;
      color: rgba(0, 0, 0, 0.8);
    }

    @media screen and (max-width: 1000px) {
      body {
        padding: 1em;
      }

      .accordion {
        width: 100%;
      }
    }
  
  	.accordion-body__contents{
      
    	p{
          
        	font-size: 14px;
            line-height: 15px;
            color: rgba(0, 0, 0, 0.8); 
          
          	span{
            	color: rgba(0, 0, 0, 0.8) !important; 	  
            }
        }
    }
}


