.newsletter-overlay {
  position: fixed;  
  z-index: 99;
  @media only screen and (min-width: 600px) {
    width: 400px;
    right: 10px;
    bottom: 10px;
  }
  @media only screen and (max-width: 599px) {
    left: 0;
    right: 0;
    bottom: 0;
  }
  
  .newsletter-popup {
    width: 87.5%;
    padding: 20px;
    background-color: #fa0d3d;
    color: #fff;
    margin: 0 auto;
    box-shadow: 0 0 5px rgba(0,0,0,0.2);
    @media only screen and (min-width: 600px) {
      width: 100%;
      margin: 0;
    }
    
    .icon__close {
      position: absolute;
      top: 28px;
      right: 50px;
      color: #fff;
      z-index: 2;
      @media only screen and (min-width: 600px) {
        right: 20px;
      }
      
      svg {
        display: block;
        width: 12px;
      }
    }
    form {
      position: relative;
      padding-bottom: 0;
      margin: 0;
      
      input[type="text"],
      input[type="email"] {
        width: 100%;
        padding: 10px 20px;
        background-color: transparent;
        border: 1px solid #fff !important;
        border-radius: 60px;
        color: #fff;
        margin-bottom: 15px;
        font-size: 14px;
        
        &::-webkit-input-placeholder { 
          color: #fff;
          opacity: 1;
        }
        &:-ms-input-placeholder { 
          color: #fff;
          opacity: 1;
        }
        &::placeholder {
          color: #fff;
          opacity: 1;
        }
      }
      button {
        font-size: 14px;
        padding: 10px 20px;
        border: 1px solid #fff !important;
        background-color: #fff;
        color: #fa0d3d;
        width: 100%;
        margin-bottom: 15px !important;
        border-radius: 60px;
      }      
    }
    .text__close {
      display: inline-block;
      padding: 2px 0;
      color: #fff;
      font-size: 14px;
      border-bottom: 1px solid #fff;
      -webkit-transition: all 0.4s;
      transition: all 0.4s;
      
      &:hover {
        border-bottom-color: transparent;
      }
    }
    .form--head__icon,
    .form--head {
      margin-bottom: 20px;
      
      h2 {
        color: #fff;
        font-size: 26px;
        margin-bottom: 20px;
        line-height: 30px;
        text-transform: uppercase;
        padding-right: 15px;
      }
    }
    .form--head__icon {      
      
      &.icon-with__space {
        padding-left: 50px;
        position: relative;
        
        .icon__form {
          position: absolute;
          top: 0;
          left: 0;
          width: 35px;
          
          img {
            width: 100%;
            display: block;
          }
        }
      }
    }
    .form--head {
      text-align: center;
      
      &.form__thanks {
        
        p {
          margin-bottom: 0;
        }
      }
    }
  }
  .errors {
    font-family: "Matter";
    padding: 5px 0;
  }
}
